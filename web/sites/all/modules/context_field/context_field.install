<?php

/**
 * @file
 * Context Field install functionality.
 */

/**
 * Implements hook_field_schema().
 */
function context_field_field_schema() {
  $columns = array(
    'context' => array(
      'type' => 'varchar',
      'size' => 'normal',
      'length' => 128,
      'not null' => FALSE,
    ),
  );
  return array(
    'columns' => $columns,
  );
}
